<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" stylesheets="@ai-style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="farmingdale.collocate.AIController">
   <children>
      <TextField fx:id="promptField" layoutX="268.0" layoutY="132.0" onAction="#updatePrompt" prefHeight="43.0" prefWidth="543.0" promptText="Enter prompt here...">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="returnButton" layoutX="936.0" layoutY="632.0" mnemonicParsing="false" onAction="#returnToCalendar" prefHeight="33.0" prefWidth="90.0" text="Return">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
