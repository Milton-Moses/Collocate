<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1080.0" styleClass="anchor-pane" stylesheets="@calendar-style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="farmingdale.collocate.CalendarController">
   <children>
      <Button fx:id="logoutButton" layoutX="931.0" layoutY="630.0" mnemonicParsing="false" onAction="#logOut" prefHeight="43.0" prefWidth="104.0" styleClass="logout-button" text="Logout...">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Group layoutX="53.0" layoutY="473.0">
         <children>
            <Rectangle arcHeight="30.0" arcWidth="30.0" fill="#a9a9a9" height="200.0" opacity="0.5" stroke="DARKGRAY" strokeType="INSIDE" width="200.0" />
            <VBox alignment="TOP_CENTER" layoutX="12.0" layoutY="15.0" prefHeight="170.0" prefWidth="175.0" spacing="20.0" styleClass="buttons-box">
               <children>
                  <Button fx:id="clientButton" mnemonicParsing="false" prefHeight="29.0" prefWidth="104.0" styleClass="clients-button" text="Clients">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="newEventButton" mnemonicParsing="false" prefHeight="20.0" prefWidth="134.0" styleClass="event-button" text="New Event">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button fx:id="aiButton" mnemonicParsing="false" onAction="#goToAi" prefHeight="17.0" prefWidth="146.0" styleClass="ai-button" text="AI Assistance">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </Group>
   </children>
</AnchorPane>
